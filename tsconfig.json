{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@/components": [
        "src/components/*"
      ],
      "@/layout": [
        "src/layout/*"
      ],
      "@/styles": [
        "src/styles/*"
      ],
      "@/posts": [
        "src/pages/posts/*"
      ],
      "@/icons": [
        "src/icons/*"
      ],
      "@/services": [
        "src/services/*"
      ],
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "extends": "astro/tsconfigs/strict"
}